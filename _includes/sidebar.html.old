<div class="page">
	<h4>December</h4>
	{% for post in site.posts %}
		{% if {{ post.date | date: "%Y-%m" }} == {{ site.time | date: "%Y" | append:"-12" }} %}
			<a href="{{ site.baseurl }}{{ post.url }}">{{ post.title }} - <small>{{ post.date | date_to_long_string }}</small></a><br />
		{% endif %}
	{% endfor %}
